@Article{Culver2019,
  author          = {Culver, Daniel A and Behr, Jürgen and Belperio, John A and Corte, Tamera J and de Andrade, Joao A and Flaherty, Kevin R and Gulati, Mridu and Huie, Tristan J and Lancaster, Lisa H and Roman, Jesse and Ryerson, Christopher J and Kim, Hyun J},
