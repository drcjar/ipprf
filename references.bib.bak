@Article{Han2014,
  author          = {Han, MeiLan K. and Zhou, Yueren and Murray, Susan and Tayob, Nabihah and Noth, Imre and Lama, Vibha N. and Moore, Bethany B. and White, Eric S. and Flaherty, Kevin R. and Huffnagle, Gary B. and Martinez, Fernando J. and Investigators, C. O. M. E. T.},
  title           = {Lung microbiome and disease progression in idiopathic pulmonary fibrosis: an analysis of the COMET study.},
  doi             = {10.1016/S2213-2600(14)70069-4},
  issn            = {2213-2619},
  issue           = {7},
  pages           = {548--556},
  pubstate        = {ppublish},
  volume          = {2},
  abstract        = {The role of the lung microbiome in the pathogenesis of idiopathic pulmonary fibrosis is unknown. We investigated whether unique microbial signatures were associated with progression of idiopathic pulmonary fibrosis. Patients (aged 35-80 years) with idiopathic pulmonary fibrosis within 4 years of diagnosis from the Correlating Outcomes with biochemical Markers to Estimate Time-progression (COMET) in idiopathic pulmonary fibrosis study were followed up for a maximum of 80 weeks. Progression-free survival was defined as time to death, acute exacerbation, lung transplant, or decrease in forced vital capacity (FVC) of 10% or greater or decrease in diffusion capacity of the lung (DLCO) of 15% or greater. DNA was isolated from 55 samples of bronchoscopic alveolar lavage. 454 pyrosequencing was used to assign operational taxonomic units (OTUs) to bacteria based on a 3% sequence divergence. Adjusted Cox models were used to identify OTUs that were significantly associated with progression-free survival at a p<0.10. These OTUs were then used in the analysis of the principal components. The association between principal components and microbes with high factor loadings and progression-free survival were assessed with Cox regression analyses. The COMET study is registered with ClinicalTrials.gov, number NCT01071707. Mean FVC was 70.1% (SD 17.0) and DLCO 42.3% (14.0) of predicted. Disease progression was significantly associated with increased relative abundance of two OTUs-Streptococcus OTU 1345 (relative risk 1.11, 95% CI 1.04-1.18; p=0.0009) and Staphylococcus OTU 1348 (1.16, 1.03-1.31, p=0.012). Thresholds for relative abundance of each OTU associated with progression-free survival were more than 3.9% for Streptococcus OTU 1345 (10.19, 2.94-35.35; p=0.0002) and more than 1.8% for Staphylococcus OTU 1348 (5.06, 1.71-14.93; p=0.003). These preliminary data suggest progression of idiopathic pulmonary fibrosis is associated with the presence of specific members within the Staphylococcus and Streptococcus genera. Additional research will be needed to identify the specific bacterial species and to ascertain whether this is a causal association. National Institutes of Health.},
  chemicals       = {DNA, Bacterial},
  citation-subset = {IM},
  completed       = {2015-03-30},
  country         = {England},
  investigator    = {Dushay, Kevin and Kurtis, Jonathon and Chapman, Jeffrey T and Anstrom, Kevin and Brown, Kevin K and Cosgrove, Gregory and Solomon, Joshua and Swigris, Jeffrey and Fernandez-Perez, Evans and Criner, Gerard and Cordova, Francis and Patel, Namrata and Rogers, Thomas and Belperio, John and King, Talmadge E and Collard, Harold R and Noth, Imre and Brown, Cathy and Garcia, Joe G N and Hogarth, D Kyle and Huang, Yong and Lussier, Yves and Ma, Shwu-Fan and Wade, Michael and Martinez, Fernando J and Flaherty, Kevin R and Toews, Galen B and White, Eric S and Hogaboam, Cory and Lama, Vibha and Moore, Bethany and Moore, Thomas and Murray, Susan and Spino, Cathie and Loyd, James E},
