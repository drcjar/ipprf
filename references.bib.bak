  keywords        = {Asbestos; Asbestosis, complications, epidemiology; Dose-Response Relationship, Drug; Environmental Exposure; Humans; Lung Neoplasms, epidemiology, etiology; Mesothelioma, epidemiology, etiology; Risk; Schools},
  month           = jan,
