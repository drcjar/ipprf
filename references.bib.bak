@Article{Yang2019a,
  author          = {Yang, Yang and Wang, Huanan and Kouadir, Mohammed and Song, Houhui and Shi, Fushan},
  title           = {Recent advances in the mechanisms of NLRP3 inflammasome activation and its inhibitors.},
  journal         = {Cell death \& disease},
