@Article{Martinez2005,
  Title                    = {The clinical course of patients with idiopathic pulmonary fibrosis.},
  Author                   = {Fernando J Martinez and Sharon Safrin and Derek Weycker and Karen M Starko and Williamson Z Bradford and Talmadge E King and Kevin R Flaherty and David A Schwartz and Paul W Noble and Ganesh Raghu and Kevin K Brown and I. P. F. Study Group},
