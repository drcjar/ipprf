  issn-linking    = {1073-449X},
  keywords        = {Alveolitis, Extrinsic Allergic, diagnosis, genetics; Biopsy, methods; Diagnosis, Differential; Female; Gene Expression, genetics, physiology; Gene Expression Profiling, methods; Humans; Immunity, genetics; Inflammation, genetics; Lung, surgery; Male; Middle Aged; Oligonucleotide Array Sequence Analysis, methods; Pulmonary Fibrosis, diagnosis, genetics; T-Lymphocytes, physiology},
  nlm-id          = {9421642},
  owner           = {NLM},
  pii             = {200504-644OC},
  pmc             = {PMC2662988},
  pmid            = {16166619},
  pubmodel        = {Print-Electronic},
  pubstatus       = {ppublish},
  revised         = {2018-11-13},
}

@Article{Kufe2009,
  author          = {Kufe, Donald W},
  title           = {Mucins in cancer: function, prognosis and therapy.},
  journal         = {Nature reviews. Cancer},
  year            = {2009},
